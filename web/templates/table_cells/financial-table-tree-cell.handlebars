
<div class="ember-table-cell-container" {{bind-attr style="view.paddingStyle"}}>
  <span {{bind-attr class=":ember-table-toggle-span view.row.isLeaf::ember-table-toggle view.row.isCollapsed:ember-table-expand:ember-table-collapse"}}
    {{action "toggleCollapse" view.row}}
        style="text-align:center;">
        {{#unless view.row.isLeaf}}
            {{#if view.row.isCollapsed}} &#9654; {{else}}  &#9660; {{/if}}
        {{/unless}}
  </span>
  <span class="ember-table-content">
    {{view.cellContent}}
  </span>
</div>

